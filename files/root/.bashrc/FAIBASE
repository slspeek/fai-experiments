#!/bin/bash


alias logs='cd /var/log/fai/localhost/last'
alias ntp='ntpdate ntp.xs4all.nl'
alias vga='lspci|grep -i vga'
alias bft='dpkg-reconfigure console-setup'
alias vd='vi /etc/dhcp/dhcpd.conf; systemctl restart isc-dhcp-server.service'
alias vh='vi /etc/hosts'
alias faic='cd /srv/fai/config'
